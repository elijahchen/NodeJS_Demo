<link rel="stylesheet" href="app.css">

<h1>The Posts Page</h1>

<% for(var i = 0; i < posts.length; i++){ %>
    <li>
        <strong>
            <%= posts[i].title %> -
        </strong>
        <%= posts[i].author %>
    </li>
<% }%>

<br>

<% posts.forEach(function (post) { %>
    <li>
        <strong>
            <%= post.title %>
        </strong>
        - <%= post.author %>
    </li>
<% })%>